    "from ftplib import FTP\n",
    "\n",
    "def list_ftp_files(host, username, password):\n",
    "    with FTP(host) as ftp:\n",
    "        ftp.login(username, password)\n",
    "\n",
    "        files = ftp.nlst()\n",
    "\n",
    "        for file in files:\n",
    "            print(file)"
   ]
